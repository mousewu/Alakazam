<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Alakazam - Unsupervised Indoor Localization System for CSE 646</title>
  <link href="http://quake0day.github.com/Alakazam/"/>
  <link rel="self" href="http://quake0day.github.com/Alakazam/feed/index.xml" type="application/atom+xml" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://quake0day.github.com/Alakazam/</id>
  <author>
    <name>quake0day</name>
    <email>schen23@buffalo.edu</email>
  </author>

  {% for post in site.posts %}
    <entry>
      <id>http://quake0day.github.com/Alakazam/{{ post.id }}</id>
      <link type="text/html" rel="alternate" href="http://quake0day.github.com/Alakazam/{{ post.url }}"/>
      <title>{{ post.title | xml_escape }}</title>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %}

</feed>
