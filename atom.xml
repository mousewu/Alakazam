---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Alakazam</title>
 <link href="http://quake0day.github.com/Alakazam/atom.xml" rel="self"/>
 <link href="http://quake0day.github.com/Alakazam/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://tom.preston-werner.com/</id>
 <author>
   <name>quake0day</name>
   <email>quake0day@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://quake0day.github.com/Alakazam/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://quake0day.github.com/Alakazam/{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
